<div id="Container">
    <div id="posts">
    <% if session[:user_id] %>
    <form action="/" method="POST">
        <input type="text" name="title" placeholder="What is this about"><br>
        <input type="text" name="content" placeholder="Write a message...">
        <input type="submit" value="submit">
    </form>
    </div>

    <div id="coworkers">
        <% if session[:user_id] %>
        <input type=search placeholder='Search'><br>
        <% end %>
        <h1> <% User.all.pluck(:name).each do |n| %> <a href="/profile/<%= n%>"><%= n %></a><br> <% end %></h1>
    </div>
</div>
<% end %>
